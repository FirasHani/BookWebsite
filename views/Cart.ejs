<body>
    <h1>Cart</h1>
    <% show.forEach(item=> { %>
        <% item.forEach(test=> { %>
        <td> <%= test.ProductName %></td>
        <td> <%= test.ProductPrice %></td>
        <td> <%= test.ProductAmount %></td>
        <td><%= test.ProductType %></td>
        <td><%= test._id %></td>
            <br>
        <% } )%>
<% } )%>

